local function createButtonTween(button)
    local tweenInfo = TweenInfo.new(0.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
    local goal = {BackgroundColor3 = Color3.fromRGB(100, 100, 100)} -- Change color when clicked

    local tween = TweenService:Create(button, tweenInfo, goal)
    return tween
end

local function isEmoji(c)
    local code = utf8.codepoint(c)  

    
    if (code >= 0x1F600 and code <= 0x1F64F) or  
       (code >= 0x1F300 and code <= 0x1F5FF) or  
       (code >= 0x1F680 and code <= 0x1F6FF) or  
       (code >= 0x1F700 and code <= 0x1F77F) or  
       (code >= 0x1F900 and code <= 0x1F9FF) or  
       (code >= 0x2600 and code <= 0x26FF) or    
       (code >= 0x2700 and code <= 0x27BF) or    
       (code >= 0x1F1E6 and code <= 0x1F1FF) then 
        return true  
    end
    return false  
end




local function modifyText(input)
    local replacements = {
        ass = "a\xEF\xBF\xB8s\xEF\xBF\xB8\xEF\xBF\xB8\xEF\xBF\xB8s\xEF\xBF\xB8\xEF\xBF\xB8\xEF\xBF\xB8",
        fuck = "f\xEF\xBF\xB8\xEF\xBF\xB8\xEF\xBF\xB8\xEF\xBF\xB8\xEF\xBF\xB8\xEF\xBF\xB8u\xEF\xBF\xB8c\xEF\xBF\xB8k\xEF\xBF\xB8\xEF\xBF\xB8\xEF\xBF\xB8",
        damn = "d\xEF\xBF\xB8\xEF\xBF\xB8\xEF\xBF\xB8\xEF\xBF\xB8\xEF\xBF\xB8\xEF\xBF\xB8a\xEF\xBF\xB8\xEF\xBF\xB8m\xEF\xBF\xB8\xEF\xBF\xB8n",
        shit = "s\xEF\xBF\xB8\xEF\xBF\xB8\xEF\xBF\xB8\xEF\xBF\xB8\xEF\xBF\xB8\xEF\xBF\xB8h\xEF\xBF\xB8i\xEF\xBF\xB8t\xEF\xBF\xB8\xEF\xBF\xB8\xEF\xBF\xB8",
        porn = "p\xEF\xBF\xB8\xEF\xBF\xB8\xEF\xBF\xB8\xEF\xBF\xB8\xEF\xBF\xB8\xEF\xBF\xB8o\xEF\xBF\xB8r\xEF\xBF\xB8n\xEF\xBF\xB8\xEF\xBF\xB8\xEF\xBF\xB8",
        bitch = "b\xEF\xBF\xB8i\xEF\xBF\xB8t\xEF\xBF\xB8\xEF\xBF\xB8\xEF\xBF\xB8c\xEF\xBF\xB8h\xEF\xBF\xB8",
        nigg = "n\xEF\xBF\xB8i\xEF\xBF\xB8g\xEF\xBF\xB8\xEF\xBF\xB8\xEF\xBF\xB8\xEF\xBF\xB8\xEF\xBF\xB8\xEF\xBF\xB8g\xEF\xBF\xB8\xEF\xBF\xB8\xEF\xBF\xB8",
        dick = "d\xEF\xBF\xB8i\xEF\xBF\xB8\xEF\xBF\xB8c\xEF\xBF\xB8k\xEF\xBF\xB8\xEF\xBF\xB8\xEF\xBF\xB8",
        gay = "g\xEF\xBF\xB8a\xEF\xBF\xB8\xEF\xBF\xB8y\xEF\xBF\xB8\xEF\xBF\xB8\xEF\xBF\xB8",
        sex = "s\xEF\xBF\xB8e\xEF\xBF\xB8\xEF\xBF\xB8x\xEF\xBF\xB8\xEF\xBF\xB8\xEF\xBF\xB8",
        ["0"] = "ï¼\xEF\xBF\xB8",
        ["1"] = "ï¼‘\xEF\xBF\xB8",
        ["2"] = "ï¼’\xEF\xBF\xB8",
        ["3"] = "ï¼“\xEF\xBF\xB8",
        ["4"] = "ï¼”\xEF\xBF\xB8",
        ["5"] = "ï¼•\xEF\xBF\xB8",
        ["6"] = "ï¼–\xEF\xBF\xB8",
        ["7"] = "ï¼—\xEF\xBF\xB8",
        ["8"] = "ï¼˜\xEF\xBF\xB8",
        ["9"] = "ï¼™\xEF\xBF\xB8",
    }

    local lowerInput = input:lower()

    -- Count non-brace characters
    local count = 0
    for i = 1, #input do
        local char = input:sub(i, i)
        if char ~= "{" and char ~= "}" then
            count = count + 1
        end
    end

    -- Adjust space insertion based on count
    local small, medium, high = false, false, false
    if count < 10 then
        small = true
    elseif count >= 10 and count < 15 then
        medium = true
    elseif count >= 15 then
        high = true
    end

    -- Replace specific words
    for word, replacement in pairs(replacements) do
        lowerInput = lowerInput:gsub(word, "{" .. replacement .. "}")
    end

    -- Handle emojis (assuming isEmoji function is defined elsewhere)
    local emojiPattern = "[%z\1-\127\194-\244][\128-\191]*"
    local modifiedInput = lowerInput:gsub(emojiPattern, function(c)
        if isEmoji(c) then
            return "{" .. c .. "}"
        end
        return c
    end)

    -- Process characters for the final result
    local result = ""
    local insideReplacedWord = false

    for i = 1, #modifiedInput do
        local char = modifiedInput:sub(i, i)

        if char == "{" then
            insideReplacedWord = true
        elseif char == "}" then
            insideReplacedWord = false
        elseif insideReplacedWord then
            result = result .. char
        elseif char == " " then
            result = result .. "\xEF\xBF\xB8\b\xEF\xBF\xB8"
        else
            result = result .. char
            -- Add spaces based on small, medium, and high conditions
            if i < #modifiedInput and modifiedInput:sub(i + 1, i + 1) ~= " " then
                    result = result .. "\xEF\xBF\xB8"
                end
            end
        end

    -- Remove braces
    result = result:gsub("{", ""):gsub("}", "")

    -- Send result in chat
    local A_1 = result
    local A_2 = "All"

    if game:GetService("TextChatService"):FindFirstChild("TextChannels") then
        game:GetService("TextChatService").TextChannels.RBXGeneral:SendAsync(A_1)
    else
        game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(A_1, A_2)
    end
end
    

    
    
    


chat.MouseButton1Click:Connect(function()
    local inputString = TextBax.Text
    modifyText(inputString)

    
    local tween = createButtonTween(chat)
    tween:Play()

    
    tween.Completed:Connect(function()
        chat.BackgroundColor3 = Color3.fromRGB(50, 50, 50) -- Reset to original color
    end)
end)

